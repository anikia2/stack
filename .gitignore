#include<iostream>
using namespace std;
#define SIZE 10


class Stack {
  int data_[SIZE];
	int top_;
	

public:
	Stack(){
		top_=0;
	}
	
	void push(int val){
		if(full()){
			cout << "OOOOPS!! STACK is full..." << endl;
			return;
		}
		data_[top_++]=val;
	}
	int pop(void){
		if(empty()){
			cout << "OOOOPS!! STACK is empty..." << endl;
			return 0;
		}
		return data_[--top_];
	}
	
	bool empty(){
		return top_==0;
	}
	bool full(){
		return top_==SIZE;
	}
		
};

int main(){
	Stack st;
	for(int i =0;i<SIZE ;i++ ){
		st.push(i*10);
	}
	while(!st.empty()){
		cout << st.pop() << endl;
	}

	return 0;
}